{
  "name": "stable-subgraph",
  "version": "0.1.0",
  "repository": "https://github.com/saleel/stable",
  "scripts": {
    "create": "graph create stable --node https://api.thegraph.com/deploy/",
    "create-local": "graph create stable --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy stable --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy stable --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "deploy:aurora": "npx truffle compile && npx truffle migrate --network aurora"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.23.2",
    "@graphprotocol/graph-ts": "^0.23.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
